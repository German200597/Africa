import json  
from pprint import pprint
import operator
def reader():
    with open('newsafr.json', encoding='utf-8') as f:
        data = json.load(f)
    global new_data    
    new_data = data['rss']['channel']['items']    
# pprint((data['rss']['channel']['items'][0]))  


# pprint(new_data)


def sorter():
    counter = {}
    for new in new_data:
        news=list(new['description'].split())
        count=0
        for word in news: 
            if len(word)>6:
                if word in counter:
                    count+=1
                    counter[word] = int(count)
                else:
                    count=1
                    counter[word] = count
    global result                
    result=sorted(counter.items(), key=operator.itemgetter(1), reverse=True) 
def top():    
    count=0 
    for i in result:
        count+=1
        if count<=10:
            print(i[0])
reader()            
sorter()    
top()        



    







 


    